{
    "name": "cypresslab/git-walrus",
    "description": "An easy to use git repository browser",
    "autoload": {
        "psr-0": {
            "GitWalrus": "src"
        }
    },
    "require": {
        "symfony/process": "2.2.*",
        "symfony/filesystem": "2.2.*",
        "symfony/yaml": "2.2.*",
        "symfony/routing": "2.2.*",
        "symfony/config": "2.2.*",
        "symfony/http-foundation": "2.2.*",
        "symfony/http-kernel": "2.2.*",
        "symfony/dependency-injection": "2.2.*",
        "cypresslab/gitelephant": "0.9.*",
        "symfony/console": "2.2.*",
        "twig/twig": "1.12.*",
        "symfony/class-loader": "2.2.*",
        "symfony/framework-bundle": "2.2.*",
        "symfony/twig-bundle": "2.2.*",
        "symfony/form": "2.2.*",
        "kriswallsmith/assetic": "@stable",
        "thomaspark/bootswatch": "*@dev",
        "cypresslab/pygments-elephant-bundle": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "mockery/mockery": "*",
        "codeception/codeception": "@stable",
        "phpmd/phpmd": "1.4.*"
    },
